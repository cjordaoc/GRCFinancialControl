<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:GRCFinancialControl.Avalonia.ViewModels"
             xmlns:loc="clr-namespace:App.Presentation.Localization;assembly=App.Presentation"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="GRCFinancialControl.Avalonia.Views.AppMasterDataView"
             x:DataType="vm:AppMasterDataViewModel">
    <TabControl SelectedItem="{Binding SelectedSection}">
        <TabItem Header="{loc:Loc Key=FINC_Navigation_MasterData_RankMappings}">
            <ContentControl Content="{Binding RankMappingsView}"/>
        </TabItem>
    </TabControl>
</UserControl>
