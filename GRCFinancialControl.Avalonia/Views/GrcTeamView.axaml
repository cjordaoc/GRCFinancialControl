<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:GRCFinancialControl.Avalonia.ViewModels"
             xmlns:views="using:GRCFinancialControl.Avalonia.Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="GRCFinancialControl.Avalonia.Views.GrcTeamView"
             x:DataType="vm:GrcTeamViewModel">
    <TabControl>
        <TabItem Header="PAPD">
            <views:PapdView DataContext="{Binding Papd}"/>
        </TabItem>
        <TabItem Header="Managers">
            <views:ManagersView DataContext="{Binding Managers}"/>
        </TabItem>
    </TabControl>
</UserControl>
