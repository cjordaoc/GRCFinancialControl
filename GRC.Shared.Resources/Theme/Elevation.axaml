<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <!-- Elevation scale expressed through box shadows for reuse across shells. -->
  <BoxShadows x:Key="ShadowLow">0 2 4 0 #33000000</BoxShadows>
  <BoxShadows x:Key="ShadowMedium">0 4 8 0 #4D000000</BoxShadows>
  <BoxShadows x:Key="ShadowHigh">0 8 16 0 #66000000</BoxShadows>
</ResourceDictionary>
